    "qs": "^6.9.4",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "shelljs": "^0.8.4",
    "shortid": "^2.2.16",
    "tempy": "^1.0.0",
    "uuid": "^8.3.2",
    "vscode-nls": "^5.0.0",
    "yaml": "^1.10.0"
  },
  "publisher": "nocalhost"
}
